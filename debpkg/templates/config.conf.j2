# {{ description }}
# Generated by {{ package_name }}

[settings]
host={{ config.settings.host }}
port={{ config.settings.port }}
ssl_enabled={{ config.settings.ssl_enabled }}

[certificates]
ca_cert={{ install_path }}/certs/ca.crt
server_cert={{ install_path }}/certs/server.crt
server_key={{ install_path }}/certs/server.key
client_cert={{ install_path }}/certs/client.crt
client_key={{ install_path }}/certs/client.key

[logging]
log_level={{ config.logging.log_level }}
log_file={{ config.logging.log_file }}
